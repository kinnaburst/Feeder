<h1>
	<%= @feed.name %>
	<%= button_to 'Edit', edit_feed_path(@feed.id), method: :get %>
	<%= button_to 'Delete', feed_path(@feed.id), method: :delete, data: { confirm: 'Are you sure?' } %>
</h1>

<% @articles.each do |article| %>
	<article>
		<h2><%= link_to article.title, article.link %></h2>
		<p class="details">Written by <%= article.author %> on <%= article.posted %></p>
		<p><%= raw article.snippet %></p>
	</article>
<% end %>