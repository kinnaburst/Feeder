<h1>My Feed List</h1>

<ul>
<% @feeds.each do |feed| %>
	<li>
		<%= link_to feed.name, feed_path(feed, username: @user.username) %>
	  <%= button_to 'Edit', edit_feed_path(feed, username: @user.username), method: :get %>
	  <%= button_to 'Delete', feed_path(feed, username: @user.username), method: :delete, data: { confirm: 'Are you sure?' } %>
	</li>
<% end %>
</ul>